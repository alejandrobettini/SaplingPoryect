    "version": "0.2.0",
    "configurations": [
        {
            "name": "ACA PONES EL NOMBRE DE LA FEATURE",
            "type": "node",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "${workspaceRoot}/node_modules/cucumber/bin/cucumber-js",
            "args": [
                "${workspaceRoot}/{PATH OSEA RUTA HACIA EL ARCHIVO}/{NOMBRE DE LA FEATURE}.feature",
            ]
        },
        {
            "name": "pruebas en AMs",
            "type": "node",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "${workspaceRoot}/node_modules/cucumber/bin/cucumber-js",
            "args": [
                "${workspaceRoot}/features/sapling.feature",
            ]
        }
    ]
}